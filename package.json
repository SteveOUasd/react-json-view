{
  "name": "react-json-view",
  "description": "Interactive react component for displaying javascript arrays and JSON objects.",
  "version": "1.14.1",
  "main": "dist/main.js",
  "files": [
    "dist/"
  ],
  "dependencies": {
    "flux": "^3.1.3",
    "react-base16-styling": "^0.5.3",
    "react-textarea-autosize": "^5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-node-env-inline": "^0.2.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "html-webpack-plugin": "2.30.1",
    "ignore-styles": "^5.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^10.1.0",
    "mocha": "^3.2.0",
    "node": "8.4.0",
    "node-sass": "^4.5.3",
    "nyc": "^10.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.6",
    "sinon": "^2.2.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "^2.9.7"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^15.5.4",
    "react-dom": "^16.0.0 || ^15.5.4"
  },
  "scripts": {
    "build": "webpack --config webpack/webpack.config.js -p --display-error-details --progress --optimize-minimize",
    "build:demo": "webpack --config webpack/webpack.config-demo.js -p --display-error-details --progress --optimize-minimize",
    "dev:hot": "webpack-dev-server --config webpack/webpack.config-dev.js --open",
    "modules:debug": "./docker/debug.sh",
    "modules:tree": "webpack --config webpack/webpack.config.js --json ",
    "modules:size-analyzer": "webpack --config webpack/webpack.config.js --json | webpack-bundle-size-analyzer",
    "start": "node server.js",
    "test": "nyc --reporter=html --reporter=text-lcov --reporter=text mocha test/**/*-test.js",
    "test:coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:watch": "nyc --reporter=html --reporter=text-lcov --reporter=text mocha -w test/**/*-test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mac-s-g/react-json-view.git"
  },
  "keywords": [
    "array-viewer",
    "base-16",
    "component",
    "interactive",
    "interactive-json",
    "json",
    "json-component",
    "json-display",
    "json-tree",
    "json-view",
    "json-viewer",
    "json-inspector",
    "json-tree",
    "react",
    "react-component",
    "react-json",
    "theme",
    "tree",
    "tree-view",
    "treeview"
  ],
  "license": "MIT",
  "author": "Mac Gainor",
  "bugs": {
    "url": "https://github.com/mac-s-g/react-json-view/issues"
  },
